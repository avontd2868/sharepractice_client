<div id="navbar-inner">
  <h1>My Profile</h1>
</div>

<div class='container'>
  <div id='user-profile'>
    <ul>
    <li><h1><%= "#{@current_user.first_name} #{@current_user.last_name}" %></h1></li>
    <li><%= "Degree: #{@current_user.degree}" %></li>
    <li><%= "Specialties: #{@current_user.specialties.first}" %></li>
    <li><%= "Locations: #{@current_user.locations.first}" %></li>
    <li><%= "Websites: #{@current_user.websites.first}" %></li>
    <li><%= "Email: #{@current_user.email}" %></li>
    <li><%= "ID: #{@current_user.id}" %></li>
    </ul>
  </div id='user-profile'>
</div>