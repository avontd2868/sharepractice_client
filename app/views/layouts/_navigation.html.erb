<%= link_to "SharePractice", root_path, :class => 'brand' %>
<ul class="nav">
  <% if current_user %>
    <li>
    Logged in as <%= "#{current_user.first_name} #{current_user.last_name}" %>
    </li>
    <li>
    <%= link_to('Logout', logout_path) %>
    </li>
  <% else %>
    <li>
    <%= link_to('Log In', login_path)  %>
    </li>
    <li>
    <%= link_to('Sign up', signup_path)  %>
    </li>
  <% end %>
</ul>